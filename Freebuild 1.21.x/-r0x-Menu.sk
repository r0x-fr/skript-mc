							set {_slotvotekickgrief} to 0
							loop all players:
								if loop-value is not player:
									set {_votekickgrief} to loop-value's name
									make gui slot {_slotvotekickgrief} with loop-value's skull named "&7%loop-value's name%":
										close player's inventory
										make player execute command "/votekick %{_votekickgrief}% Grief / Vol"
									add 1 to {_slotvotekickgrief}
									make gui slot 26 with red stained glass named "&cRetour":
										make player execute command "/menu vote kick"
						open the last gui for the player
					if arg 3 is "pub":
						create a gui with virtual chest inventory with 3 row named "&4VoteKick &7- &cChoix du joueur":
							set {_slotvotekickpub} to 0
							loop all players:
								if loop-value is not player:
									set {_votekickpub} to loop-value's name
									make gui slot {_slotvotekickpub} with loop-value's skull named "&7%loop-value's name%":
										close player's inventory
										make player execute command "/votekick %{_votekickpub}% Publicité"
