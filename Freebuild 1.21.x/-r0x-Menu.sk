#VERIFICATION DE QUANTITE EN MODE PROMPT (POUR LA BANQUE)
on chat:
	if {prompt.quantity.%player's UUID%} is true:
		if player is not in world "Events" or "Jeux":
			cancel event
			wait 1 tick
			set {_payment.value} to message parsed as integer
			if {_payment.value} > 0:
				make player execute command "/menu economie envoi %{_payment.value}%"
			else:
				send "&cVous devez entrer un chiffre ou un nombre ! -> Opération annulée"
			set {prompt.quantity.%player's UUID%} to false

# ON EXECUTE LA COMMANDE /menu QUAND ON CLIC SUR LE LIVRE r0x.fr - Menu
on inventory click:
	if player is not in world "Events" or "Jeux":
		if event-item is a written book named "&2r0x.fr - Menu":
			cancel event
			if player has permission "r0x.menu":
				make player execute command "/menu"
				stop
			close player's inventory
			message "&cVous n'avez pas la permission d'aller dans le menu, complétez d'abord le tutoriel du bateau !" to player


# GROUPE DE RUSTINES POUR NE PAS FAIRE DISPARAITRE LE MENU
#
# ON EVITE QUE LE JOUEUR RECUPERE LE LIVRE (même si osef) QUAND IL MEURT, PUIS LE REFAIRE SPAWN AU RESPAWN
on death of player:
	if victim is not in world "Events" or "Jeux":
		remove 1 written book named "&2r0x.fr - Menu" from the drops
on respawn:
	if player is not in world "Events" or "Jeux":
		set slot 17 of player's inventory to a written book named "&2r0x.fr - Menu"
# CONNEXION DANS LE MONDE FREEBUILD, LE JOUEUR RECUPERE PREMIER MENU
on join:
	if player is not in world "Events" or "Jeux":
		set slot 17 of player's inventory to a written book named "&2r0x.fr - Menu"
# CHANGEMENT DE MONDE, ON REMET LE LIVRE SI JAMAIS L'INVENTAIRE EST SEPARE
on player world change:
	if player is not in world "Events" or "Jeux":
		set slot 17 of player's inventory to a written book named "&2r0x.fr - Menu"


# ON REMET LE MENU R0X.FR APRES UN CLEAN DE L'INVENTAIRE
on command "/eclear" or "/clear" or "/clearinventory" or "/ci" or "/eci" or "/eclearinventory" or "/clearinvent" or "/eclearinvent":
	if player is not in world "Events" or "Jeux":
		if player has permission "r0x.menu":
			wait 1 tick
			set slot 17 of player's inventory to a written book named "&2r0x.fr - Menu"
			wait 2 ticks
			send "&4[&6Menu&4] &2Item de menu remis en place dans l'inventaire."

# PROTECTION POUR PAS AVOIR DES BOURSES VIA SHOPKEEPER (marchand acheteur) EN CREATIVE
on command "/shopkeeper" or "/shopkeepers":
	if gamemode of player is creative:
		if player don't have permission "r0x.admin":
			cancel event
			message "&cImpossible d'invoquer un marchand en mode créatif !" to player

# PROTECTION POUR PAS UTILISER L'HDV EN CREATIVE hdv,ca,ah,crazyauction,crazyauctions
on command "/ca" or "/ah" or "/hdv" or "/crazyauction" or "/crazyauctions":
	if gamemode of player is creative:
		if player don't have permission "r0x.admin":
			cancel event
			message "&cImpossible d'accéder à l'hotel des ventes en mode creatif !" to player