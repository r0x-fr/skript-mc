					make gui slot 4 with {_itemlandmenu}:
						make player execute command "/land menu"
					make gui slot 6 with eye of ender named "&c/land view" with lore "&aAffiche les informations et" and "&ales contours du territoire":
						close player's inventory
						make player execute command "/land view"
					make gui slot 8 with wooden axe named "&c/land selection" with lore "&aActive le mode sélection" and "&apour les territoires et" and "&ales sous-zones":
						close player's inventory
						make player execute command "/land selection"
					make gui slot 9 with respawn anchor named "&c/land setspawn" with lore "&aDéfini le spawn de" and "&avotre territoire":
						close player's inventory
						make player execute command "/land setspawn"
					make gui slot 11 with green bed named "&c/land spawn" with lore "&aVous téléporte au spawn" and "&ade votre territoire":
						close player's inventory
						make player execute command "/land spawn"
					make gui slot 13 with green concrete named "&2Protéger un chunk" with lore "&aProtège le chunk où" and "&avous vous trouvez":
						close player's inventory
						make player execute command "/land claim"
					make gui slot 15 with {_itemlandunclaim}:
						close player's inventory
						make player execute command "/land unclaim"
					make gui slot 17 with birch sign named "&eTutoriels":
						make player execute command "/menu land info"
					make gui slot 22 with red stained glass named "&cRetour":
						make player execute command "/menu"
