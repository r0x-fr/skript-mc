command /menu [<text>] [<text>] [<text>]:
	permission: r0x.menu
	trigger:
		if player is not in world "Events" or "Jeux":
			# POUR UNE RAPIDITE ACCRUE, D'ABORD ON VERIF SI IL Y A UN ARGUMENT
			# POUR UNE RAPIDITE ACCRUE, D'ABORD ON VERIF SI IL Y A UN ARGUMENT
			# POUR UNE RAPIDITE ACCRUE, D'ABORD ON VERIF SI IL Y A UN ARGUMENT
			# POUR UNE RAPIDITE ACCRUE, D'ABORD ON VERIF SI IL Y A UN ARGUMENT
			# POUR UNE RAPIDITE ACCRUE, D'ABORD ON VERIF SI IL Y A UN ARGUMENT
			# 
			# SOUS-MENU 1 - Règles
			if arg 1 is "rules":
				create a gui with virtual chest inventory with 2 rows named "&eFreebuild &7- &4Règles":
					set {_itemreglestriche} to a paper named "&4Triche / Usebug"
					set line 1 of lore of {_itemreglestriche} to "&aMods de triches et usebugs sont interdits"
					set line 2 of lore of {_itemreglestriche} to "&aUn seul usebug autorisé : &caccès au toit du Nether"
					set line 3 of lore of {_itemreglestriche} to "&aMods : X-Ray, Aimbot, Anti-KB, NoFall, WorldDownloader..."
					set line 4 of lore of {_itemreglestriche} to "&2Sanction : &4Ban définitif"
					make gui slot 0 with {_itemreglestriche} 
					set {_itemreglespub} to a paper named "&4Publicité"
					set line 1 of lore of {_itemreglespub} to "&aEnvoi d'IP ou nom de serveur,"
					set line 2 of lore of {_itemreglespub} to "&aque ce soit sur le tchat ou en message privé"
					set line 3 of lore of {_itemreglespub} to "&2Sanction : &4Ban définitif"
					make gui slot 1 with {_itemreglespub} 
					set {_itemreglescomportement} to a paper named "&cComportement correct"
					set line 1 of lore of {_itemreglescomportement} to "&aPas d'insultes, harcèlement ou manque de respect"
					set line 2 of lore of {_itemreglescomportement} to "&aFlood, spam, majuscules, déco-reco excessifs"
					set line 3 of lore of {_itemreglescomportement} to "&2Sanction : &6Selon gravité, selon le modérateur"
					make gui slot 2 with {_itemreglescomportement} 
					set {_itemregleselevages} to a paper named "&6Elevages trop importants"
					set line 1 of lore of {_itemregleselevages} to "&aDe plus de 100 entités dans une même zone"
					set line 2 of lore of {_itemregleselevages} to "&2Sanction : &6Kill des entités dans la zone"
					make gui slot 3 with {_itemregleselevages} 
					set {_itemreglesredstone} to a paper named "&6Boucles de Redstone"
					set line 1 of lore of {_itemreglesredstone} to "&aBoucle qui tourne seule sans arrêt"
					set line 2 of lore of {_itemreglesredstone} to "&2Sanction : &6Suppression complète du système"
					make gui slot 4 with {_itemreglesredstone} 
					set {_itemreglesaccusation} to a paper named "&6Accusations sans preuves"
					set line 1 of lore of {_itemreglesaccusation} to "&aDemandez de l'aide aux &5Ambassadeurs"
					set line 2 of lore of {_itemreglesaccusation} to "&aplutôt que d'accuser quelqu'un sans savoir"
					set line 3 of lore of {_itemreglesaccusation} to "&2Sanction : &6Selon gravité, selon le modérateur"
					make gui slot 5 with {_itemreglesaccusation} 
					set {_itemreglesdoublecompte} to a paper named "&4Double-comptes"
					set line 1 of lore of {_itemreglesdoublecompte} to "&aQue ce soit pour jouer plus facilement ou"
					set line 2 of lore of {_itemreglesdoublecompte} to "&apour contourner une sanction"
					set line 3 of lore of {_itemreglesdoublecompte} to "&2Sanction : &4Ban définitif des deux comptes"
					make gui slot 6 with {_itemreglesdoublecompte} 
					set {_itemreglesdegradationmap} to a paper named "&cDégradation volontaire de la carte"
					set line 1 of lore of {_itemreglesdegradationmap} to "&aArbres volants, incendies de grosses surfaces"
					set line 2 of lore of {_itemreglesdegradationmap} to "&adestruction de constructions du serveur"
					set line 3 of lore of {_itemreglesdegradationmap} to "&2Sanction : &cBan temporaire (1 semaine)"
					make gui slot 7 with {_itemreglesdegradationmap} 
					set {_itembuildinnapproprié} to a paper named "&6Construction innappropriée"
					set line 1 of lore of {_itembuildinnapproprié} to "&aPouvant porter à atteinte à une personne"
					set line 2 of lore of {_itembuildinnapproprié} to "&aou à un groupe de personnes"
					set line 3 of lore of {_itembuildinnapproprié} to "&2Sanction : &6Selon gravité, selon le modérateur"
					make gui slot 8 with {_itembuildinnapproprié} 
					set {_itempseudoinnapproprié} to a paper named "&6Pseudonyme innapproprié"
					set line 1 of lore of {_itempseudoinnapproprié} to "&aPouvant porter à atteinte à une personne"
					set line 2 of lore of {_itempseudoinnapproprié} to "&aou à un groupe de personnes"
					set line 3 of lore of {_itempseudoinnapproprié} to "&2Sanction : &6Selon gravité, selon le modérateur"
					make gui slot 9 with {_itempseudoinnapproprié} 
					set {_itemantiafk} to a paper named "&6Anti-AFK automatique"
					set line 1 of lore of {_itemantiafk} to "&aTous interdits"
					set line 2 of lore of {_itemantiafk} to "&asauf les usines à pêche"
					set line 3 of lore of {_itemantiafk} to "&2Sanction : &6Emprisonnement de 30 minutes"
					make gui slot 10 with {_itemantiafk} 
					set {_itemgriefvol} to a paper named "&cGrief / Vol"
					set line 1 of lore of {_itemgriefvol} to "&aDétruire des constructions qui ne sont pas"
					set line 2 of lore of {_itemgriefvol} to "&aà soi ou piller ressources, coffres..."
					set line 3 of lore of {_itemgriefvol} to "&2Sanction : &6Emprisonnement de 60 minutes"
					make gui slot 11 with {_itemgriefvol}
					set {_itemabusmecaniques} to a paper named "&cDétournement des mécaniques"
					set line 1 of lore of {_itemabusmecaniques} to "&aTout abus ou détournement des mécaniques"
					set line 2 of lore of {_itemabusmecaniques} to "&aproposées (ponts / herses / portes...)"
					set line 3 of lore of {_itemabusmecaniques} to "&2Sanction : &6destruction système, ou autre selon modérateur"
					make gui slot 12 with {_itemabusmecaniques} 
					make gui slot 17 with red stained glass named "&cRetour":
						make player execute command "/menu"
				open the last gui for the player
				stop
			# 
			# SOUS-MENU 2 - VoteKick / VoteDay
			if arg 1 is "vote":
				create a gui with virtual chest inventory with 1 row named "&eFreebuild &7- &fLancer un vote":
					make gui slot 1 with clock named "&eLancer un VoteDay":
						close player's inventory
						make player execute command "/voteday"
					make gui slot 4 with netherite sword named "&4Lancer un VoteKick":
						make player execute command "/menu vote kick"
					make gui slot 7 with red stained glass named "&cRetour":
						make player execute command "/menu"
				open the last gui for the player
				# VOTEKICK : Choix du motif
				if arg 2 is "kick":
					create a gui with virtual chest inventory with 1 row named "&4VoteKick &7- &cMotif":
						make gui slot 0 with paper named "&cFlood / Spam du tchat":
							make player execute command "/menu vote kick chat"
						make gui slot 1 with paper named "&cManque de respect / insultes":
							make player execute command "/menu vote kick insultes"
						make gui slot 2 with paper named "&cGrief / Vol":
							make player execute command "/menu vote kick grief"
						make gui slot 3 with paper named "&cPublicité":
							make player execute command "/menu vote kick pub"
						make gui slot 7 with red stained glass named "&cRetour":
							make player execute command "/menu vote"
					open the last gui for the player
					# VOTEKICK : Choix du joueur
					if arg 3 is "chat":
						create a gui with virtual chest inventory with 3 row named "&4VoteKick &7- &cChoix du joueur":
							set {_slotvotekickchat} to 0
							loop all players:
								if loop-value is not player:
									set {_votekickchat} to loop-value's name
									make gui slot {_slotvotekickchat} with loop-value's skull named "&7%loop-value's name%":
										close player's inventory
										make player execute command "/votekick %{_votekickchat}% Flood / Spam du tchat"
									add 1 to {_slotvotekickchat}
									make gui slot 26 with red stained glass named "&cRetour":
										make player execute command "/menu vote kick"
						open the last gui for the player
					if arg 3 is "insultes":
						create a gui with virtual chest inventory with 3 row named "&4VoteKick &7- &cChoix du joueur":
							set {_slotvotekickinsultes} to 0
							loop all players:
								if loop-value is not player:
									set {_votekickinsultes} to loop-value's name
									make gui slot {_slotvotekickinsultes} with loop-value's skull named "&7%loop-value's name%":
										close player's inventory
										make player execute command "/votekick %{_votekickinsultes}% Manque de respect / insultes"
									add 1 to {_slotvotekickinsultes}
									make gui slot 26 with red stained glass named "&cRetour":
										make player execute command "/menu vote kick"
						open the last gui for the player
					if arg 3 is "grief":
