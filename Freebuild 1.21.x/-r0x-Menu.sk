						make gui slot 1 with green concrete named "&2Scoreboard : &aActivé" with lore "&7Cliquez pour : &cdésactiver":
							close player's inventory
							make player execute command "/scboard off"
					if {scoreboard-xyz.%player's UUID%} is false:
						make gui slot 4 with compass named "&2Coordonnées : &cMasquées" with lore "&7Cliquez pour : &aactiver":
							close player's inventory
							make player execute command "/scboard xyz"
					if {scoreboard-xyz.%player's UUID%} is not set:
						make gui slot 4 with compass named "&2Coordonnées : &aAffichées" with lore "&7Cliquez pour : &cdésactiver":
							close player's inventory
							make player execute command "/scboard xyz"
					make gui slot 7 with red stained glass named "&cRetour":
						make player execute command "/menu"
				open the last gui for the player
				stop
			# 
			# SOUS-MENU 4 - Téléportation
			if arg 1 is "tp":
				create a gui with virtual chest inventory with 3 rows named "&eFreebuild &7- &5Téléportation":
					set {_slottp} to 0
					loop all players:
						if loop-value is not player:
							set {_playerheadtp} to loop-value's skull named "&7%loop-value's name%"
							set line 1 of lore of {_playerheadtp} to "&aClic-Gauche : &cse téléporter à"
							set line 2 of lore of {_playerheadtp} to "&aClic-Droit : &ctéléporter à vous"
							set {_playernametp} to loop-value's name
							make gui slot {_slottp} with {_playerheadtp}:					
								if gui-click-type is left mouse button:
									make player execute command "/tpa %{_playernametp}%"
									close player's inventory
								else if gui-click-type is right mouse button:
									make player execute command "/tpahere %{_playernametp}%"
									close player's inventory
							add 1 to {_slottp}
					if player has permission "essentials.tptoggle":
						if {tptoggle.%player's UUID%} is not set:
							make gui slot 24 with green concrete named "&2Cliquez pour &cdésactiver&2 la téléportation":					
								make player execute command "/tptoggle"
								set {tptoggle.%player's UUID%} to false
								make player execute command "/menu tp"
						else if {tptoggle.%player's UUID%} is false:
							make gui slot 24 with red concrete named "&2Cliquez pour &aactiver &2la téléportation":					
								make player execute command "/tptoggle"
								delete {tptoggle.%player's UUID%}
								make player execute command "/menu tp"
					else:
						make gui slot 24 with barrier named "&cActiver/Désactiver la téléportation" with lore "&7Rang requis : &aVillageois" 
					if player has permission "essentials.back":
						set {_itemback} to player head named "&cRetourner à l'emplacement précédent"
						add "{SkullOwner:{Id:""af0c4f24-76b8-414d-8b5f-6b529dae3971"",Properties:{textures:[{Value:""eyJ0ZXh0dXJlcyI6eyJTS0lOIjp7InVybCI6Imh0dHA6Ly90ZXh0dXJlcy5taW5lY3JhZnQubmV0L3RleHR1cmUvNDdlNTA1OTFmNDExOGI5YWU0NDc1NWY3YjQ4NTY5OWI0YjkxN2YwMGQ2NWY1ZWE4NTUzZWU0ODgyNmQyMzRjNyJ9fX0=""}]}}}" to nbt of {_itemback}
						make gui slot 25 with {_itemback}:
							make player execute command "/back"
					else:
						make gui slot 25 with barrier named "&cRetourner à l'emplacement précédent" with lore "&7Rang requis : &aVillageois" 
